{
    "experiment": {
        "name": "densenet169_combo_imagenet_transfer_medico"
    },
    "model": {
        "build_file": "densenet169_combo",
        "train": false,
        "weights": "imagenet",
        "models": [
            "split_0_densenet169_imagenet_transfer_medico_model.h5",
            "split_1_densenet169_imagenet_transfer_medico_model.h5",
            "split_2_densenet169_imagenet_transfer_medico_model.h5"
        ]
    },
    "dataset": {
        "link": "medico.json"
    },
    "optimizer": {
        "name": "sgd",
        "params": {
            "nesterov": true,
            "lr": 0.0004,
            "momentum": 0.9
        }
    },
    "hyper_parameters": {
        "epochs": 50,
        "batch_size": 20,
        "freeze_layers": 0,
        "patience": 5,
        "class_weights": true,
        "activity_regularizer": {
            "name": "l1_l2",
            "params": {
                "l1": 0.001,
                "l2": 0.001
            }
        }
    },
    "image_processing": {
        "image_width": 299,
        "image_height": 299,
        "image_channels": 3,
        "training_data_generator": "basic",
        "validation_data_generator": "basic"
    }
}