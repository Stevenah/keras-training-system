{
    "experiment": {
        "name": "densenet169_imagenet_transfer_medico"
    },
    "model": {
        "build_file": "densenet169",
        "weights": "imagenet"
    },
    "dataset": {
        "link": "medico.json"
    },
    "optimizer": {
        "name": "sgd",
        "params": {
            "nesterov": true,
            "lr": 0.0004,
            "momentum": 0.9
        }
    },
    "hyper_parameters": {
        "epochs": 150,
        "batch_size": 16,
        "freeze_layers": 0,
        "patience": 5,
        "class_weights": true,
        "activity_regularizer": {
            "name": "l1_l2",
            "params": {
                "l1": 0.001,
                "l2": 0.001
            }
        }
    },
    "evaluation": {
        "path": "/home/stevenah/Pictures/Medico_2018_test_set"
    },
    "image_processing": {
        "image_width": 224,
        "image_height": 224,
        "image_channels": 3,
        "training_data_generator": "basic",
        "validation_data_generator": "basic"
    }
}