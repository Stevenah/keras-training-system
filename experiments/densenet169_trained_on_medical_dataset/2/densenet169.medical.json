{
    "experiment": {
        "name": "densenet169_trained_on_medical_dataset"
    },
    "model": {
        "build_file": "densenet169_pooling"
    },
    "dataset": {
        "link": "lapgyn4.cataract.json"
    },
    "optimizer": {
        "name": "nadam",
        "params": {
            "lr": 4e-5
        }
    },
    "hyper_parameters": {
        "epochs": 150,
        "batch_size": 16,
        "freeze_layers": 0,
        "patience": 10,
        "class_weights": false
    },
    "image_processing": {
        "image_width": 340,
        "image_height": 340,
        "image_channels": 3,
        "training_data_generator": "custom",
        "validation_data_generator": "custom"
    }
}